  <row>
    <panel>
      <title>Top 10 Referring Domains</title>
      <table>
        <search>
          <query>index=* sourcetype="access_combined" referer!="-" referer!="*gosplunk.com*" | top limit=10 referer</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">10</option>
        <option name="drilldown">none</option>
        <format type="color" field="percent">
          <colorPalette type="minMidMax" maxColor="#EC9960" midColor="#708794" minColor="#006D9C"></colorPalette>
          <scale type="minMidMax" midType="percentile" midValue="80"></scale>
        </format>
      </table>
    </panel>
    <panel>
      <title>Top 10 non Search Engine Referrers</title>
      <table>
        <search>
          <query>index=* sourcetype="access_combined" referer!="-" referer!="*gosplunk.co*" referer!="*baidu.*" referer!="*google.*" referer!="*bing*" referer!="*yahoo.*" referer!="*yahoojp*" referer!="*baid" referer!="*duckduckgo.co*" referer!="http://site.ru" referer!="http://*-imperia.com/" referer!="*yandex*" referer!="http://pizza-*.com/" 
| top limit=10 referer</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">50</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
        <format type="color" field="percent">
          <colorPalette type="minMidMax" maxColor="#EC9960" midColor="#708794" minColor="#006D9C"></colorPalette>
          <scale type="minMidMax" midType="percentile" midValue="80"></scale>
        </format>
      </table>
    </panel>
    <panel>
      <title>Top 10 Pages Accessed</title>
      <table>
        <search>
          <query>index=* sourcetype="access_combined" (uri_path!="/robots.txt" uri_path!="*/wp-*" uri_path!="//*" uri_path!=/ uri_path!="/50EE9C4E*" uri_path!="/favicon.ico" uri_path!="/ads.txt" uri_path!="/feed/" ) 
| eval URL=urldecode(uri_path) 
| top limit=10 URL</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">10</option>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <format type="color" field="percent">
          <colorPalette type="minMidMax" maxColor="#53A051" minColor="#FFFFFF"></colorPalette>
          <scale type="minMidMax"></scale>
        </format>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>400 &amp; 500 Errors</title>
      <table>
        <title>Default Time Range: Previous Month</title>
        <search>
          <query>index=* sourcetype="access_combined" status&gt;=400
| iplocation clientip
| stats latest(_time) as Date count(status) as count by uri, status, Country, Region, City
| convert ctime(Date)
| sort - status count</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
        <format type="color" field="Country">
          <colorPalette type="sharedList"></colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
        <format type="color" field="status">
          <colorPalette type="sharedList"></colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
        <format type="color" field="uri">
          <colorPalette type="sharedList"></colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
      </table>
    </panel>
    <panel>
      <title>Data Consumers with Geographic Information</title>
      <table>
        <title>Default Time Range: Previous Month</title>
        <search>
          <query>index=* sourcetype="access_combined"
| stats sum(bytes) as bytes by clientip
| iplocation clientip
| eval MB=round(bytes/1024/1024,4)
| fields clientip Country Region City MB
| sort - MB</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">10</option>
